{
  "name": "feeld-automation-project",
  "version": "1.0.0",
  "scripts": {
    "test": "npx wdio run wdio.conf.ts",
    "test:login": "npx wdio run wdio.conf.ts --spec src/tests/login.test.ts",
    "test:forms": "npx wdio run wdio.conf.ts --spec src/tests/forms.test.ts",
    "test:home": "npx wdio run wdio.conf.ts --spec src/tests/home.test.ts",
    "test:all": "npx wdio run wdio.conf.ts --spec src/tests/**/*.test.ts",
    "local:test": "env-cmd npm test",
    "local:login": "env-cmd npm run test:login",
    "local:forms": "env-cmd npm run test:forms",
    "local:home": "env-cmd npm run test:home",
    "local:all": "env-cmd npm run test:all"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@wdio/appium-service": "^8.16.13",
    "@wdio/browserstack-service": "^9.21.0",
    "@wdio/cli": "^8.16.13",
    "@wdio/globals": "^8.16.13",
    "@wdio/local-runner": "^8.16.13",
    "@wdio/mocha-framework": "^8.16.13",
    "@wdio/spec-reporter": "^8.16.13",
    "appium": "^3.1.2",
    "appium-uiautomator2-driver": "^6.6.2",
    "appium-xcuitest-driver": "^10.9.1",
    "env-cmd": "^10.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "webdriverio": "^8.16.13"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
